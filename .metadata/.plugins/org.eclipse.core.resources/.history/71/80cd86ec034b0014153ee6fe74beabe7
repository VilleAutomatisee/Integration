package Ville;

public class PositionBloc {
	
	private int position;
	private PositionBloc suivant;
	
	
	public PositionBloc(int position, PositionBloc suivant) {
		this.position = position;
		this.suivant = suivant;
	}
	
	
	public PositionBloc(int v) 
	{
		this.position = v;
		this.suivant = null;
	}
	
	public int getPosition() 
	{
		return position;
	}
	
	public void setPosition(int position) 
	{
		this.position = position;
	}
	
	public void ajouterAuDebut(int position) 
	{
		PositionBloc ancienPremier= position;
		premier = new PositionBloc(position,ancienPremier);
	}
}
